{
  "name": "@gomoku/api",
  "description": "Gomoku TypeScript API package",
  "apiVersion": "1",
  "version": "0.0.1",
  "author": "Vitaly Kuprin (vitas_s@inbox.lv); Aleksandrs Vaguscenko (aleksandrs.vaguscneko@gmail.com)",
  "license": "AGPL-3.0",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "download": "sh ./scripts/download-openapi-document.sh",
    "download:localhost": "sh ./scripts/download-openapi-document.sh localhost",
    "download:ci": "sh ./scripts/download-openapi-document.sh CI",
    "codegen": "node scripts/codegen.js"
  },
  "type": "module",
  "dependencies": {
    "@gomoku/eslint-config": "*",
    "@tanstack/react-query": "^5.62.0",
    "openapi-fetch": "^0.13.0",
    "openapi-react-query": "^0.2.5",
    "openapi-typescript": "^7.4.3"
  }
}
