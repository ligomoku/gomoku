{
  "name": "@gomoku/api",
  "description": "Gomoku TypeScript API package",
  "apiVersion": "1",
  "version": "0.0.1",
  "author": "Vitaly Kuprin (vitas_s@inbox.lv); Aleksandrs Vaguscenko (aleksandrs.vaguscneko@gmail.com)",
  "license": "AGPL-3.0",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "download": "sh ./scripts/download-openapi-document.sh",
    "download:localhost": "sh ./scripts/download-openapi-document.sh localhost",
    "download:ci": "sh ./scripts/download-openapi-document.sh CI",
    "codegen": "kubb generate"
  },
  "dependencies": {
    "@gomoku/eslint-config": "*",
    "@kubb/cli": "^3.2.0",
    "@kubb/core": "^3.2.0",
    "@kubb/plugin-react-query": "^3.2.0",
    "@kubb/plugin-ts": "^3.2.0",
    "@kubb/swagger": "^2.28.4",
    "@kubb/swagger-client": "^2.28.4",
    "@kubb/swagger-tanstack-query": "^2.28.4",
    "@tanstack/react-query": "^5.62.7"
  }
}
