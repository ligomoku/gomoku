{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["turbo.json", "package.json"],
  "tasks": {
    "build": {
      "outputs": ["dist/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "check-types": {
      "dependsOn": ["^check-types"],
      "outputs": []
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "typecheck": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
